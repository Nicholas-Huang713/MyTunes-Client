m\" onSubmit={handleOnSubmit} noValidate validated={validated}>\n            <div className=\"reg-form-title\">Register Here</div>\n            {errMsg !== '' && <span style={{color: 'red'}}>{errMsg}</span>}\n            <div className=\"reg-form-content\">\n                {/* <Form.Group> */}\n                <Form.Group>\n                    <Form.Label>First Name</Form.Label>\n                    <Form.Control \n                        // id=\"firstname\"\n                        required\n                        type=\"text\" \n                        placeholder=\"Enter first name\" \n                        value={firstName}\n                        onChange={(e) => setFirstName(e.target.value)}\n                        minLength=\"3\"\n                    />\n                    {/* <Form.Control.Feedback></Form.Control.Feedback> */}\n                    <Form.Control.Feedback type=\"invalid\">\n                        Please provide a valid first name. (Minimum: 3 characters)\n                    </Form.Control.Feedback>\n                </Form.Group>\n                <Form.Group>\n                    <Form.Label>Last Name</Form.Label>\n                    <Form.Control  \n                        // id=\"lastname\"\n                        required\n                        minLength=\"3\"\n                        type=\"text\" \n                        placeholder=\"Enter last name\"\n                        value={lastName}\n                        onChange={(e) => setLastName(e.target.value)}\n                    />\n                    {/* <Form.Control.Feedback>Looks good!</Form.Control.Feedback> */}\n                    <Form.Control.Feedback type=\"invalid\">\n                        Please provide a valid last name. (Minimum: 3 characters)\n                    </Form.Control.Feedback>\n                </Form.Group>\n                <Form.Group>\n                    <Form.Label>UserName</Form.Label>\n                    <Form.Control \n               